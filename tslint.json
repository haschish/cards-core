{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "indent": [true, "spaces", 2],
      "quotemark": false,
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "no-empty": false,
      "max-classes-per-file": false,
      "trailing-comma": false
    },
    "rulesDirectory": []
}
